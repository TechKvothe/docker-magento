<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">cellshop_minerva_faq_listing.cellshop_minerva_faq_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <deps>
            <dep>cellshop_minerva_faq_listing.cellshop_minerva_faq_listing_data_source</dep>
        </deps>
        <spinner>cellshop_minerva_faq_columns</spinner>
    </settings>
    <dataSource name="cellshop_minerva_faq_listing_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <updateUrl path="mui/index/render"/>
        </settings>
        <aclResource>Cellshop_Minerva::faq</aclResource>
        <dataProvider name="cellshop_minerva_faq_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <columns name="cellshop_minerva_faq_columns">
        <column name="id">
            <settings>
                <label translate="true">ID</label>
            </settings>
        </column>
        <column name="question">
            <settings>
                <label translate="true">Question</label>
            </settings>
        </column>
        <column name="answer">
            <settings>
                <label translate="true">Answer</label>
            </settings>
        </column>
        <column name="is_published">
            <settings>
                <label translate="true">Is published</label>
            </settings>
        </column>
        <column name="created_at">
            <settings>
                <label translate="true">Created at</label>
            </settings>
        </column>
        <column name="updated_at">
            <settings>
                <label translate="true">Updated at</label>
            </settings>
        </column>
    </columns>
</listing>
